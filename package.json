{
  "name": "vscode-move-file",
  "displayName": "Move File",
  "description": "Move files with fuzzy search folder picker",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "moveFile.move",
        "title": "Move File to Folder"
      },
      {
        "command": "moveFile.handleTab",
        "title": "Handle Tab for Move File"
      }
    ],
    "keybindings": [
      {
        "command": "moveFile.handleTab",
        "key": "tab",
        "when": "inQuickOpen && inFilesMovePickerContext"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "keywords": ["file", "move", "folder", "fuzzy search", "navigation"],
  "author": "alarbada",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alarbada/vscode-move-file"
  },
  "bugs": {
    "url": "https://github.com/alarbada/vscode-move-file/issues"
  },
  "homepage": "https://github.com/alarbada/vscode-move-file#readme"
}
